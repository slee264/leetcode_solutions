# 
# https://leetcode.com/problems/fibonacci-number/description/
#

class Solution:
    def fib(self, n: int) -> int:
        def fib_helper(n: int):
            if n == 0:
                return 0
            if n == 1:
                return 1
            return fib_helper(n-1) + fib_helper(n-2)

        return fib_helper(n)
